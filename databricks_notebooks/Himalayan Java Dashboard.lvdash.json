    {
      "name": "f423c94b",
      "displayName": "hourly sales in major lalitpur city",
      "queryLines": [
        "-- hourly sales trends\r\n",
        "SELECT start AS hour_start, end AS hour_end, branch, hourly_revenue\r\n",
        "FROM hj_orders.gold.hourly_sales_trends\r\n",
        "WHERE branch in ('Satdobato', 'Jawalakhel', 'Imadol', 'Sanepa International Club', 'Kumaripati')\r\n",
        "ORDER BY hour_start ASC;\r\n"
      ]
    },
    {
      "name": "d1e3e0ae",
